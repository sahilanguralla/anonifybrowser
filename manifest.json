{
	"manifest_version": 2,

	"name": "Anonify Browser",
	"description": "Lets you hide your firefox from websites.",
	"version": "0.1",

	"options_page": "options.html",
	"browser_action": {
		"default_icon": "images/logo-128.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"tabs",
		"webRequest",
		"webRequestBlocking",
		"<all_urls>"
	],
	"background":{
		"page":"background.html"
	}
}
